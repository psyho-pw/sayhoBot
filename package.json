{
  "name": "sayho-bot",
  "version": "0.0.1",
  "description": "Discord music bot",
  "author": "sayho",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=${npm_config_env:-development} nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint -c eslint.config.mjs . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "env:up": "oci os object put -bn environments --file .env/.env.development --name sayho-bot/.env/.env.development --no-multipart && oci os object put -bn environments --file .env/.env.production --name sayho-bot/.env/.env.production --no-multipart",
    "env:down": "oci os object get -bn environments --file .env/.env.development --name sayho-bot/.env/.env.development && oci os object get -bn environments --file .env/.env.production --name sayho-bot/.env/.env.production"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/voice": "^0.19.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@snazzah/davey": "^0.1.9",
    "@toss/nestjs-aop": "^2.1.7",
    "@ybd-project/ytdl-core": "^6.0.8",
    "axios": "^1.10.0",
    "bgutils-js": "^3.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "discord.js": "^14.25.1",
    "express-session": "^1.18.2",
    "ffmpeg-static": "^5.3.0",
    "helmet": "^8.1.0",
    "jsdom": "^27.3.0",
    "micromatch": "^4.0.8",
    "mysql2": "^3.16.0",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^6.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "simple-youtube-api": "^5.2.1",
    "sodium-native": "^5.0.10",
    "typeorm": "^0.3.28",
    "typeorm-transactional": "^0.5.0",
    "undici": "^7.16.0",
    "uuid": "^12.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jest": "30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/micromatch": "^4.0.10",
    "@types/node": "25.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "discord-api-types": "^0.38.37",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jest": "30.2.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest-setup.ts"
    ]
  }
}
