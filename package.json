{
  "name": "sayho-bot",
  "version": "0.0.1",
  "description": "Discord music bot",
  "author": "sayho",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=${npm_config_env:-development} nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "env:up": "oci os object put -bn environments --file .env/.env.development --name sayho-bot/.env/.env.development --no-multipart && oci os object put -bn environments --file .env/.env.production --name sayho-bot/.env/.env.production --no-multipart",
    "env:down": "oci os object get -bn environments --file .env/.env.development --name sayho-bot/.env/.env.development && oci os object get -bn environments --file .env/.env.production --name sayho-bot/.env/.env.production"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/voice": "^0.19.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@snazzah/davey": "^0.1.9",
    "@ybd-project/ytdl-core": "^6.0.8",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "discord.js": "^14.25.1",
    "express-session": "^1.18.2",
    "ffmpeg-static": "^5.3.0",
    "helmet": "^8.1.0",
    "micromatch": "^4.0.8",
    "mysql2": "^3.16.0",
    "nest-winston": "^1.10.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "simple-youtube-api": "^5.2.1",
    "sodium-native": "^5.0.10",
    "typeorm": "^0.3.28",
    "typeorm-transactional": "^0.5.0",
    "undici": "^7.16.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "29.5.14",
    "@types/micromatch": "^4.0.9",
    "@types/node": "22.8.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "discord-api-types": "^0.37.120",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "29.7.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
